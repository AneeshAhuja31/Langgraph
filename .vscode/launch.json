{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug All Pytest Tests",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["AGENTIC-RAG-FLOW"],
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
